# Created by Vercel CLI
NEXT_PUBLIC_BASE_URL="http://localhost:3000"

# Dashboard/Console URL (for login button)
NEXT_PUBLIC_DASHBOARD_URL="https://console.tambo.co"

# External service URLs (optional - defaults provided)
NEXT_PUBLIC_DOCS_URL="https://docs.tambo.co"
NEXT_PUBLIC_LICENSE_URL=
NEXT_PUBLIC_PRIVACY_URL=
NEXT_PUBLIC_TERMS_URL=

# PostHog Analytics (optional - omit for local/OSS deployments)
# Uses reverse proxy at /ingest for more accurate tracking
NEXT_PUBLIC_POSTHOG_KEY=

# Clay webhook for contact form submissions
CLAY_WEBHOOK_URL=

# Cloudflare Turnstile (spam protection for contact form)
# Get keys at https://dash.cloudflare.com/?to=/:account/turnstile
# For local dev, use test keys:
#   Site key (always passes): 1x00000000000000000000AA
#   Secret key (always passes): 1x0000000000000000000000000000000AA
NEXT_PUBLIC_CLOUDFLARE_TURNSTILE_SITE_KEY=
CLOUDFLARE_TURNSTILE_SECRET_KEY=

