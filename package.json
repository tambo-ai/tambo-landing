{
  "name": "tambo-website",
  "description": "Build agents that speak your UI",
  "version": "1.4.5",
  "license": "MIT",
  "type": "module",
  "scripts": {
    "dev": "npm-run-all --parallel watch:styles dev:next",
    "dev:next": "next dev --webpack",
    "dev:turbo": "next dev",
    "watch:styles": "bun --watch ./styles/scripts/setup-styles.ts",
    "dev:https": "bun --watch ./styles/scripts/setup-styles.ts & next dev --experimental-https",
    "build": "bun run setup:styles && next build --webpack",
    "build:analyze": "cross-env SOURCE_MAPS=true bun run build && source-map-explorer .next/static/chunks/*.js",
    "build:profile": "cross-env NEXT_BUILD_PROFILE=true next build",
    "start": "next start",
    "lint": "biome lint --max-diagnostics=200",
    "lint:fix": "biome lint --write --max-diagnostics=200",
    "format": "biome format --write .",
    "analyze": "cross-env ANALYZE=true next build",
    "typecheck": "tsc --noEmit --incremental false",
    "validate:env": "bun ./libs/validate-env.ts",
    "cleanup:integrations": "bun ./libs/cleanup-integrations.ts",
    "setup:styles": "bun ./styles/scripts/setup-styles.ts",
    "lighthouse": "bunx @unlighthouse/cli --site http://localhost:3000",
    "setup:sanity-mcp": "echo -n 'cursor://anysphere.cursor-deeplink/mcp/install?name=Sanity&config=eyJ1cmwiOiJodHRwczovL21jcC5zYW5pdHkuaW8iLCJ0eXBlIjoiaHR0cCJ9Cg==' | pbcopy && echo 'Link copied to clipboard (without newline)' && echo 'Paste this in Cursor or browser address bar'",
    "sanity:schema-extract": "cd integrations/sanity && bun --env-file ../../.env.local sanity schema extract",
    "sanity:typegen": "cd integrations/sanity && bun --env-file ../../.env.local sanity typegen generate && bun biome check --write --unsafe"
  },
  "dependencies": {
    "@base-ui-components/react": "^1.0.0-rc.0",
    "@mdx-js/react": "^3.1.1",
    "@modelcontextprotocol/sdk": "^1.25.1",
    "@next/mdx": "^16.1.6",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "@rive-app/react-webgl2": "^4.26.2",
    "@tambo-ai/react": "^0.67.1",
    "@valibot/to-json-schema": "^1.5.0",
    "clsx": "^2.1.1",
    "dompurify": "^3.3.1",
    "feed": "^5.2.0",
    "gsap": "^3.13.0",
    "hamo": "1.0.0-dev.6",
    "highlight.js": "^11.11.1",
    "json-stringify-pretty-compact": "^4.0.0",
    "lenis": "^1.3.16",
    "lucide-react": "^0.560.0",
    "next": "^16.1.6",
    "nextra": "^4.6.1",
    "npm-run-all": "^4.1.5",
    "posthog-js": "^1.335.0",
    "postprocessing": "^6.38.0",
    "radix-ui": "^1.4.3",
    "react": "^19.2.2",
    "react-dom": "^19.2.2",
    "react-medium-image-zoom": "^5.4.0",
    "rehype-katex": "^7.0.1",
    "rehype-pretty-code": "^0.14.1",
    "remark-gfm": "^4.0.1",
    "remark-mdx-frontmatter": "^5.2.0",
    "streamdown": "^1.6.10",
    "tempus": "1.0.0-dev.17",
    "three": "^0.181.2",
    "tunnel-rat": "^0.1.2",
    "zod": "^4.2.1",
    "zod-to-json-schema": "^3.25.0",
    "zustand": "^5.0.9"
  },
  "devDependencies": {
    "@biomejs/biome": "2.3.8",
    "@csstools/postcss-global-data": "^3.1.0",
    "@next/bundle-analyzer": "16.0.9",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@theatre/core": "^0.7.2",
    "@theatre/studio": "^0.7.2",
    "@types/bun": "^1.3.4",
    "@types/node": "^25.0.0",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/three": "^0.181.0",
    "babel-plugin-react-compiler": "^1.0.0",
    "cross-env": "^10.1.0",
    "lefthook": "^2.0.9",
    "postcss-functions": "^4.0.2",
    "postcss-preset-env": "^10.5.0",
    "source-map-explorer": "^2.5.3",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3"
  },
  "trustedDependencies": [
    "@biomejs/biome",
    "@tailwindcss/oxide",
    "esbuild",
    "lefthook",
    "sharp"
  ],
  "packageManager": "bun@1.3.0"
}
